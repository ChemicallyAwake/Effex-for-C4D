			{
				CYCLE
				{
					POP_FLOCK_PREDATORS_FACTORY_PASSIVE;
					POP_FLOCK_PREDATORS_FACTORY_VORTICITY;
					POP_FLOCK_PREDATORS_FACTORY_IMPLICIT;
					POP_FLOCK_PREDATORS_FACTORY_DIFFUSE;
				}
			}
			REAL POP_FLOCK_PREDATORS_MAXVELOCITY { MIN 0.1;  STEP 0.1;}
			REAL POP_FLOCK_PREDATORS_COHESION { MIN 0.0; MAX 200.0; MAXSLIDER 100; UNIT PERCENT; CUSTOMGUI REALSLIDER; SCALE_H; }
			REAL POP_FLOCK_PREDATORS_COHESION_RADIUS { MIN 0.1; MAX 100.0; STEP 0.1; }
			REAL POP_FLOCK_PREDATORS_SEPARATION { MIN 0.0; MAX 100.0; UNIT PERCENT; CUSTOMGUI REALSLIDER; SCALE_H; }
			REAL POP_FLOCK_PREDATORS_SEPARATION_RADIUS { MIN 0.1; MAX 100.0; STEP 0.1; }
			BOOL POP_FLOCK_PREDATORS_EATPREY {}
			REAL POP_FLOCK_PREDATORS_EATPREY_RADIUS { MIN 0.1; MAX 100.0; STEP 0.1; }
		}
