			REAL POP_FLOCK_COHESION_RADIUS { MIN 0.1; MAX 100.0; STEP 0.1;}
			REAL POP_FLOCK_SEPARATION { MIN 0.0; MAX 100.0; MAXSLIDER 1000; UNIT PERCENT; CUSTOMGUI REALSLIDER; SCALE_H;}
