	REAL	XCELL_VALUE		{MIN 0; STEP 0.01; INPORT; EDITPORT; CREATEPORT; }
	VECTOR	XCELL_VALUEV		{MIN 0 0 0; INPORT; EDITPORT; CREATEPORT; }
	
	REAL	XCELL_VALUE_OUT		{MIN 0; STEP 0.01; OUTPORT; EDITPORT; CREATEPORT; }
	VECTOR	XCELL_VALUEV_OUT		{MIN 0 0 0; OUTPORT; EDITPORT; CREATEPORT; }
