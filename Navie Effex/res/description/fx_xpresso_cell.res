     {
        GVGENERALOBJECT	XCELL_CHANNEL	
	{
		INPORT; STATICPORT; CREATEPORT; NEEDCONNECTION; 
		ACCEPT { fx_channel_velocity; fx_channel_levelset; fx_volume; fx_fluid; fx_liquid; fx_channel_scalar; fx_channel_temp; fx_channel_fuel; fx_channel_fire; fx_channel_vector; }
